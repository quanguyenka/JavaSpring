<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="triangle" class="itsol.spring_h3.Triangle">
		<property name="list">
			<list>
				<ref bean="point1"/>
				<ref bean="point2"/>
				<ref bean="point3"/>
			</list>
		</property>
	</bean>

	<bean id="point1" class="itsol.spring_h3.Point">
		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>
	<bean id="point2" class="itsol.spring_h3.Point">
		<property name="x" value="50" />
		<property name="y" value="0" />
	</bean>
	<bean id="point3" class="itsol.spring_h3.Point">
		<property name="x" value="-30" />
		<property name="y" value="0" />
	</bean>
</beans>
